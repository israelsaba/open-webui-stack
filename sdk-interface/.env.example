# Anthropic API Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google Gemini API Configuration
GOOGLE_API_KEY=your_google_api_key_here

# API Keys for Bearer Token Authentication
# Format: username1:token1;username2:token2;username3:token3
# Example: API_KEYS=alice:op_wui_abc123;bob:op_wui_def456
# Tokens must start with op_wui_
API_KEYS=

# Server Configuration
HOST=0.0.0.0
PORT=8000

# Optional: Add additional configuration as needed
LOG_LEVEL=info
